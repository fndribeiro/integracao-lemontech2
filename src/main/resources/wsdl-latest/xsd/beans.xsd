<?xml version="1.0" encoding="UTF-8"?>
<!--Published by JAX-WS RI at http://jax-ws.dev.java.net. RI's version is 
	JAX-WS RI 2.2.1-b01-. -->
<xs:schema version="1.0"
	targetNamespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans"
	xmlns:tns="http://lemontech.com.br/selfbooking/wsselfbooking/beans"
	xmlns:ns11="http://lemontech.com.br/selfbooking/wsselfbooking/beans/rodoviario"
	xmlns:ns10="http://lemontech.com.br/selfbooking/wsselfbooking/beans/expense"
	xmlns:ns9="http://lemontech.com.br/selfbooking/wsselfbooking/beans/traslado"
	xmlns:ns8="http://lemontech.com.br/selfbooking/wsselfbooking/beans/seguro"
	xmlns:ns7="http://lemontech.com.br/selfbooking/wsselfbooking/beans/fornecedor"
	xmlns:ns6="http://lemontech.com.br/selfbooking/wsselfbooking/beans/outro"
	xmlns:ns5="http://lemontech.com.br/selfbooking/wsselfbooking/common/types"
	xmlns:ns4="http://lemontech.com.br/selfbooking/wsselfbooking/beans/carro"
	xmlns:ns3="http://lemontech.com.br/selfbooking/wsselfbooking/beans/hotel"
	xmlns:ns2="http://lemontech.com.br/selfbooking/wsselfbooking/beans/aereo"
	xmlns:ns1="http://lemontech.com.br/selfbooking/wsselfbooking/beans/types"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans/types"
		schemaLocation="beansTypes.xsd" />
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/common/types"
		schemaLocation="commonTypes.xsd" />
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans/aereo"
		schemaLocation="beansAereo.xsd" />
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans/hotel"
		schemaLocation="beansHotel.xsd" />
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans/carro"
		schemaLocation="beansCarro.xsd" />
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans/outro"
		schemaLocation="beansOutro.xsd" />
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans/fornecedor"
		schemaLocation="beansFornecedores.xsd" />
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans/seguro"
		schemaLocation="beansSeguro.xsd" />
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans/traslado"
		schemaLocation="beansTraslado.xsd" />
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans/expense"
		schemaLocation="beansExpense.xsd" />
	<xs:import
		namespace="http://lemontech.com.br/selfbooking/wsselfbooking/beans/rodoviario"
		schemaLocation="beansRodoviario.xsd" />
	<xs:complexType name="regionalRef">
		<xs:sequence>
			<xs:element name="codigo" type="ns5:codigoElemento50" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="regional">
		<xs:complexContent>
			<xs:extension base="tns:regionalRef">
				<xs:sequence>
					<xs:element name="descricao" type="ns5:descricaoElemento100" />
					<xs:element name="pcc" type="ns5:identificadorBackoffice" minOccurs="0" />
					<xs:element name="identificadorBackofficeCliente" type="ns5:identificadorBackoffice" minOccurs="0" />
					<xs:element name="identificadorBackofficeFranquia" type="ns5:identificadorBackoffice" minOccurs="0" />
					<xs:element name="ativo" type="xs:boolean" minOccurs="1"/>
					<xs:element name="taxaFee" type="xs:double" minOccurs="0"/>
					<xs:element name="corporateIdGol" type="ns5:corporateIdGol" minOccurs="0" />
					<xs:element name="numeroDk" minOccurs="0">
					   <xs:simpleType>
					      <xs:restriction base="xs:string">
					         <xs:maxLength value="15"></xs:maxLength>
					      </xs:restriction>
					   </xs:simpleType>
					</xs:element>
					<xs:element name="numeroDkTreinamento" minOccurs="0">
						<xs:simpleType>
						   <xs:restriction base="xs:string">
						      <xs:maxLength value="15"></xs:maxLength>
						   </xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="numeroDkEvento" minOccurs="0">
					   <xs:simpleType>
					      <xs:restriction base="xs:string">
						     <xs:maxLength value="15"></xs:maxLength>
						  </xs:restriction>
					   </xs:simpleType>
					</xs:element>
					<xs:element name="emitirNotaFiscalEmNomeDoHospede" type="xs:boolean" minOccurs="0" default="false"/>
					<xs:element name="emitirNotaFiscalEmNomeDaRegional" type="xs:boolean" minOccurs="0" default="false"/>			
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="centroDeCustoRef">
		<xs:sequence>
			<xs:element name="codigo" type="ns5:codigoElemento50">
				<xs:annotation>
					<xs:documentation>Codigo do centro de custo.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="regionalRef" type="tns:regionalRef">
				<xs:annotation>
					<xs:documentation>Regional que o centro de custo pertence.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="centroDeCusto">
		<xs:complexContent>
			<xs:extension base="tns:centroDeCustoRef">
				<xs:sequence>
					<xs:element name="descricao" type="ns5:descricaoElemento100" />
					<xs:element name="configuracao" type="tns:configuracaoCentroDeCusto" />
					<xs:element name="ativo" type="xs:boolean" minOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="subCentroDeCustoRef">
		<xs:sequence>
			<xs:element name="codigo" type="ns5:codigoElemento50" />
			<xs:element name="centroDeCustoRef" type="tns:centroDeCustoRef" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="subCentroDeCusto">
		<xs:complexContent>
			<xs:extension base="tns:subCentroDeCustoRef">
				<xs:sequence>
					<xs:element name="descricao" type="ns5:descricaoElemento100" />
					<xs:element name="configuracao" type="tns:configuracaoCentroDeCusto" />
					<xs:element name="ativo" type="xs:boolean" minOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="configuracaoCentroDeCusto">
		<xs:sequence>
			<xs:element name="autoAprovavel" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Centro de custo auto aprovavel.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="debitaBudget" type="xs:boolean" minOccurs="0" nillable="true">
				<xs:annotation>
					<xs:documentation>Permite debitar budget
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="contaContabilRef">
		<xs:sequence>
			<xs:element name="codigo" type="ns5:codigoElemento50" />
			<xs:choice>
				<xs:sequence>
					<xs:element name="regionalRef" type="tns:regionalRef">
						<xs:annotation>
							<xs:documentation>Regional que o centro de custo pertence.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				
				<xs:sequence>
					<xs:element name="aereo" type="xs:boolean" />
					<xs:element name="hotel" type="xs:boolean" />
					<xs:element name="carro" type="xs:boolean" />
					<xs:element name="seguro" type="xs:boolean" />
					<xs:element name="outro" type="xs:boolean" />
					<xs:element name="traslado" type="xs:boolean" />
					<xs:element name="rodoviario" type="xs:boolean" />
				</xs:sequence>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>

    <xs:complexType name="contaContabil">
		<xs:complexContent>
			<xs:extension base="tns:contaContabilRef">
				<xs:sequence>
					<xs:element name="descricao" type="ns5:descricaoElemento100" />
					<xs:element name="ativo" type="xs:boolean" minOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="funcionarios">
		<xs:sequence>
			<xs:element name="funcionario" type="tns:funcionario"
				nillable="true" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="funcionarioRef">
		<xs:sequence>
			<xs:element name="matricula" type="ns5:matricula" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="acompanhanteRef">
		<xs:sequence>
			<xs:element name="passageiro" type="tns:funcionarioRef" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>	

	<xs:complexType name="funcionario">
		<xs:complexContent>
			<xs:extension base="tns:funcionarioRef">
				<xs:sequence>
					<xs:element name="nome" type="ns5:descricaoElemento100" />
					<xs:element name="departamento" minOccurs="0" type="ns5:descricaoElemento100" />
					<xs:element name="cargo" minOccurs="0" type="ns5:descricaoElemento100" />
					<xs:element name="cpf" minOccurs="0" >
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="11"></xs:maxLength>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="passaporte" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="50"></xs:maxLength>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="dataNascimento" type="xs:date" minOccurs="0"/>
					<xs:element name="sexo" type="ns1:sexo" />
					<xs:choice minOccurs="0" maxOccurs="1">
						<xs:element name="centroDeCustoRef" type="tns:centroDeCustoRef" />
						<xs:element name="subCentroDeCustoRef" type="tns:subCentroDeCustoRef" />
					</xs:choice>
					<xs:element name="contato" type="tns:contato" minOccurs="0" />
					<xs:element name="login" type="tns:login" minOccurs="0"/>
					<xs:element name="bypassAprovacaoNacional" default="false" type="xs:boolean" minOccurs="0"/>
					<xs:element name="bypassAprovacaoInternacional" default="false" type="xs:boolean" minOccurs="0"/>
					<xs:element name="configuracao" type="tns:configuracaoFuncionario"/>
					<xs:element name="ativo" type="xs:boolean" minOccurs="1"/>
					<xs:element name="nomeaAgenciaPreferencial" type="ns5:descricaoElemento100" minOccurs="0"/>
					<xs:element name="atividade" minOccurs="0" type="ns5:descricaoElemento100" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="parametrizacaoCentroDeCustoAprovacaoRef">
		<xs:sequence>
			<xs:element name="funcionarioRef" type="tns:funcionarioRef" minOccurs="0" maxOccurs="1"/>
			
			<xs:choice minOccurs="1">
				<xs:element name="centroDeCustoRef" type="tns:centroDeCustoRef" />
				<xs:element name="subCentroDeCustoRef" type="tns:subCentroDeCustoRef" />
			</xs:choice>
			
			<xs:element name="nivelDeAprovacao" type="xs:int" minOccurs="0" default="0">
				<xs:annotation>
					<xs:documentation>Nivel de aprovacao, apos aprovacao desse nivel o sistema enviara a solicitacao para o proximo nivel caso tenha. Para tanto o cliente devera estar devidamente parametrizado.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tipoDeViagemDaAprocacao" type="ns1:tipoViagem" default="TODOS" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tipo de Viagem da Aprocacao. TODOS, INTERNACIONAL, INTERCONTINENTAL, CONTINENTAL E NACIONAL. Para tanto o cliente devera estar devidamente parametrizado.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tipoDePassageiro" type="ns1:tipoPassageiro" default="TODOS" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tipo de passageiro. TODOS, FUNCIONARIO e TERCEIRO.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aprovaSomenteAprovador" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aprova somente Aprovador. Quando estiver como 'true', as pendencias desse aprovador serao somente as que o passageiro tiver perfil de aprovador.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aprovaSomenteDentroDaPolitica" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aprova somente dentro da politica. Quando estiver como 'true', somente as solicitacoes dentro da politica poderao ser aprovadas pelo aprovador.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="periodoViagemInicial" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Período de viagem inicial.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="tipoDeSolicitacao" type="ns1:tipoDeSolicitacao" default="TODOS" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tipo de solicitacao. TODOS, CORPORATIVO, EXPENSE, INCENTIVO e TREINAMENTO.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="parametrizacaoCentroDeCustoAprovacao">
		<xs:complexContent>
			<xs:extension base="tns:parametrizacaoCentroDeCustoAprovacaoRef">
				<xs:sequence>
					
					<xs:element name="vencimento" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Vencimento.</xs:documentation>
						</xs:annotation>
					</xs:element>
					
					<xs:element name="periodoViagemFinal" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Período de viagem final.</xs:documentation>
						</xs:annotation>
					</xs:element>
					
					<xs:element name="valorAlcadaInicial" type="xs:double" default="0" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Valor da alçada inicial.</xs:documentation>
						</xs:annotation>
					</xs:element>
					
					<xs:element name="valorAlcadaFinal" type="xs:double" default="0" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Valor da alçada final.</xs:documentation>
						</xs:annotation>
					</xs:element>
					
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="parametrizacaoCentroDeCustoSolicitacaoRef">
		<xs:sequence>
			<xs:element name="funcionarioRef" type="tns:funcionarioRef" />
			<xs:choice>
				<xs:element name="centroDeCustoRef" type="tns:centroDeCustoRef" />
				<xs:element name="subCentroDeCustoRef" type="tns:subCentroDeCustoRef" />
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="parametrizacaoCentroDeCustoSolicitacao">
		<xs:complexContent>
			<xs:extension base="tns:parametrizacaoCentroDeCustoSolicitacaoRef">
				<xs:sequence>
					<xs:element name="vencimento" type="xs:date" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="parametrizacaoCentroDeCustoDebitoRef">
		<xs:sequence>
			<xs:element name="funcionarioRef" type="tns:funcionarioRef" />
			<xs:choice>
				<xs:element name="centroDeCustoRef" type="tns:centroDeCustoRef" />
				<xs:element name="subCentroDeCustoRef" type="tns:subCentroDeCustoRef" />
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="parametrizacaoCentroDeCustoDebito">
		<xs:complexContent>
			<xs:extension base="tns:parametrizacaoCentroDeCustoDebitoRef">
				<xs:sequence>
					<xs:element name="vencimento" type="xs:date" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="login">
		<xs:sequence>
			<xs:element name="usuario">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="3"></xs:minLength>
						<xs:maxLength value="100"></xs:maxLength>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="contato">
		<xs:sequence>
			<xs:element name="email" type="ns5:email" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Email.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ddiTelefone" type="ns5:ddi" minOccurs="0"/>
			<xs:element name="dddTelefone" type="ns5:ddd" minOccurs="0"/>
			<xs:element name="telefone" type="ns5:telefone" minOccurs="1"/>
			<xs:element name="ddiCelular" type="ns5:ddi" minOccurs="0"/>
			<xs:element name="dddCelular" type="ns5:ddd" minOccurs="0"/>
			<xs:element name="celular" type="ns5:telefone" minOccurs="0"/>
			<xs:element name="forcaAtualizacao" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="configuracaoFuncionario">
		<xs:sequence>
			<xs:element name="autoAprova" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Caso o aprovador seja o passageiro e essa opcao estiver como SIM. A solicitacao sera automaticamente aprovada no nivel do aprovador.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="solicitaParaTodos" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Quando o funcionario tiver um perfil Solicitante ou Aprovador e este campo estiver como 'true', ele podera pre aprovar todas os solicitacoes. Independente das parametrizacoes. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="preAprovaTodos" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Para perfil Aprovador e Solicitante, indeficara se poderao pre-aprovar todos os cc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="forcaDeVenda" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Quando estiver como 'true' sera pedido para validar o numero de autorizacao. Para tanto o cliente devera estar devidamente parametrizado. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nivelHierarquico" type="xs:int" minOccurs="0" default="0">
				<xs:annotation>
					<xs:documentation>Numero hierarquico do funcionario, na listagem de aprovadores aparecera somentes os aprovadores com numero inferior ao do passageiro. Para tanto o cliente devera estar devidamente parametrizado.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tipoCadastro" type="ns1:tipoCadastroFuncionario" default="SELF_BOOKING" minOccurs="0"/>
			<xs:element name="categoriaHospedagem" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Categoria de hotel a ser associada ao cadastro do funcionario.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"></xs:maxLength>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="perfilFuncionario" type="ns1:perfilFuncionario" />
			<xs:element name="perfilAereo" type="ns1:perfilAereo" minOccurs="0" />
			<xs:element name="perfilHospedagem" type="ns1:perfilHospedagem" minOccurs="0" />
			<xs:element name="visualizaSaldoBudgetAprovacao" type="xs:boolean" minOccurs="0" default="false"/>
			<xs:element name="aprovaBudgetNegativo" type="xs:boolean" minOccurs="0" default="false"/>
			<xs:element name="usuarioTerceiro" type="xs:boolean" default="false" minOccurs="0"/>
			<xs:element name="antecedenciaViagemNacional" type="xs:int" minOccurs="0"/>
			<xs:element name="antecedenciaViagemInternacional" type="xs:int" minOccurs="0"/>
			<xs:element name="politicaDespesa" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Politica de despesa a ser associada ao cadastro do funcionario.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="80"></xs:maxLength>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="bloqueiaSolicitarViagem" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bloqueio de solicitacao de viagem a ser associada ao cadastro do funcionario.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="podeSerAprovadoSemSaldoBudget" type="xs:boolean" minOccurs="0" default="false"/>
			<xs:element name="bloqueiaSolicitarViagemAdiantamento" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bloqueio de solicitacao de viagem para o produto adiantamento.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="permiteSolicitarCarro" type="xs:boolean" minOccurs="0" default="true"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="solicitacao">
		<xs:sequence>
			<xs:element name="idSolicitacao" type="xs:int" />
